# Queue Mode Configuration (from appsettings.Development.json)
QueueMode__UseInMemory=true
QueueMode__UseKafka=true
QueueMode__EnableHybridMode=true

# Kafka Settings (from appsettings.Development.json)
KafkaSettings__Enabled=true
KafkaSettings__BootstrapServers=
KafkaSettings__SaslUsername=
KafkaSettings__SaslPassword=

# Additional Kafka Configuration
KafkaSettings__SecurityProtocol=SaslSsl
KafkaSettings__SaslMechanism=Plain
KafkaSettings__GroupId=distributed-queue-consumer-group

# Logging Configuration
Logging__LogLevel__Default=Information
Logging__LogLevel__Microsoft.AspNetCore=Warning
